{
  "manifest_version": 2,
  "name": "Twitdown",
  "version": "1.0.0",
  "description": "A simple Twitter video/GIF extractor!",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "permissions": ["tabs", "http://*/", "https://*/"]
}
